<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xinlong.dao.OrderMapper">



    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xinlong.bean.Order">
        <id column="to_id" property="toId" />
        <result column="order_id" property="orderId" />
        <result column="user_id" property="userId" />
        <result column="order_price" property="orderPrice" />
        <result column="order_flag" property="orderFlag" />
        <result column="order_user" property="orderUser" />
        <result column="order_tel" property="orderTel" />
        <result column="order_addr" property="orderAddr" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        to_id, order_id, user_id, order_price, order_flag, order_user, order_tel, order_addr
    </sql>

</mapper>
