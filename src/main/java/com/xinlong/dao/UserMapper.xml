<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xinlong.dao.UserMapper">



    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xinlong.bean.User">
        <id column="tu_id" property="tuId" />
        <result column="user_id" property="userId" />
        <result column="user_nickna" property="userNickna" />
        <result column="user_pwd" property="userPwd" />
        <result column="user_email" property="userEmail" />
        <result column="user_tel" property="userTel" />
        <result column="user_flag" property="userFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        tu_id, user_id, user_nickna, user_pwd, user_email, user_tel, user_flag
    </sql>

</mapper>
